<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="478.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bookiescrape.app.sample.Controller">
  <children>
  
    <HBox alignment="CENTER" layoutX="452.0" layoutY="258.0" prefHeight="33.0" prefWidth="32.0" spacing="10.0">
      <children>
        
        <JFXButton onAction="#selectOutputExcelFilePath" prefHeight="33.0" prefWidth="17.0" text="Open">
          <cursor>
            <Cursor fx:constant="HAND" />
          </cursor>
        </JFXButton>
      
      </children>
    </HBox>
  
    <Label layoutX="14.0" layoutY="228.0" text="Select Output File">
      <font>
        <Font name="System Bold Italic" size="18.0" />
      </font>
    </Label>
    
    <Button layoutX="189.0" layoutY="305.0" mnemonicParsing="false" onAction="#startWebScrapping" text="Start Scrapping " />

    <TextField fx:id="outputExcelFilePathFeild" layoutX="31.0" layoutY="262.0" prefHeight="25.0" prefWidth="417.0" promptText="Select Output File Path where you want to store scrapped Data" />
  
  </children>
</AnchorPane>
